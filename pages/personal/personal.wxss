#header{
    width:100%;
    height: 260px;
    background: grey;
   margin-top: 380rpx;
    color:#ffffff;
}
#header .content{
    padding-top:60px;
    text-align: center;
}
#header .content image{
    width:100px;
    height:100px;
    border-radius: 100px;
}
#header .content .nickname{
    padding-left: 10px;
    padding-top:20px;
}
#header .content .orther{
    padding-top:10px;
    font-size:16px;
    color: #ebebeb;
}


.index-container{
  display: flex;
  flex-direction: column;
  align-items: center;
}

.index-container .btn{

 width: 400rpx;

  border: 1px solid #109D59;
  padding: 5px 10px;
  border-radius: 5px;
  text-align: center;
  margin-top: 40rpx;
}

.index-container .btn text{
  
  font-weight: bold;
  color: #109D59;
}